{
  "name": "test-sass-compilation",
  "version": "1.0.0",
  "description": "Test de compilation avec l'implémentaion Dart-Sass.",
  "scripts": {
    "dev": "sass --embed-source-map --source-map-urls=relative --style=expanded src/scss/style.scss:dist/css/style.css",
    "watch": "sass --watch --embed-source-map --source-map-urls=relative --style=expanded src/scss/style.scss:dist/css/style.css",
    "build": "sass --no-source-map --style=compressed src/scss/style.scss:dist/css/style.css"
  },
  "keywords": [
    "sass",
    "dart-sass",
    "css"
  ],
  "author": "Céline André",
  "devDependencies": {
    "sass": "^1.43.2"
  }
}
